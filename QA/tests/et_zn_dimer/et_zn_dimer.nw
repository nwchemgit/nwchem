start zn
title  "Zn -> Zn(+) ET "
echo

geometry zn
 Zn 0.0 0.0 0.0
symmetry c1
end

geometry zn_r10
 zn1 0.0 0.0  5.0
 zn2 0.0 0.0 -5.0
 symmetry C1
end

