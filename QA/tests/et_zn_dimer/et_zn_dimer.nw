basis "ao basis" spherical
 Zn1   library "ahlrichs vdz"
 Zn2   library "ahlrichs vdz"
end

charge 0
set geometry zn
scf
 uhf
 singlet
 vectors input atomic output zn.mo
end
task scf

charge 1
scf
 uhf
 doublet
 vectors input atomic output znp.mo
 print "mulliken ao"
end
task scf

