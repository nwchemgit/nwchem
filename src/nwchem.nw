title "Water geometry optimization in cc-pvdz basis set"

start h2o

geometry autosym
  O    0.0    0.0    -0.02
  H   -0.74   0.0    -0.76
  H    0.74   0.0    -0.76
end

basis
  H library cc-pvdz
  O library cc-pvdz
end

driver
  clear
end

scf; print low; end

task scf optimize

task scf freq

task dft optimize

task dft freq


