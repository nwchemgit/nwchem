      endif
      endif
      deriv(17,ipss)=deriv(17,ipss)+dercon*rwi1(nax+jnum)
      if(ireact.ne.0) then
      deriv(17,ipss)=deriv(17,ipss)+dercon*rffss/rwi2(nax+jnum)
      endif
