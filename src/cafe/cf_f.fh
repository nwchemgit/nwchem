      zs(isf,ix,2,ipss)=zs(isf,ix,2,ipss)+sumen2
      zs(jsf,ix,2,ipss)=zs(jsf,ix,2,ipss)+sumen2
      zs(isf,ix,3,ipss)=zs(isf,ix,3,ipss)+sumen3
      zs(jsf,ix,3,ipss)=zs(jsf,ix,3,ipss)+sumen3
   32 continue
