      do 132 ix=1,3
      do 32 jsf=1,msf
      sumen1=zero
      sumen2=zero
      sumen3=zero
      do 33 iax=1,nax
cx      if(jfal(iax).eq.jsf.and.jmal(iax).eq.1) then
